{
    "mem0": {
        "llm": {
            "provider": "env:LLM_PROVIDER",
            "config": {
                "model": "env:LLM_MODEL",
                "temperature": 0,
                "max_tokens": 4000,
                "ollama_base_url": "env:OLLAMA_BASE_URL"
            }
        },
        "embedder": {
            "provider": "env:EMBEDDER_PROVIDER",
            "config": {
                "model": "env:EMBEDDER_MODEL",
                "ollama_base_url": "env:EMBEDDER_OLLAMA_BASE_URL"
            }
        },
        "vector_store": {
            "provider": "env:VECTOR_STORE_PROVIDER",
            "config": {
                "collection_name": "env:QDRANT_COLLECTION",
                "host": "env:QDRANT_HOST",
                "port": "env:QDRANT_PORT",
                "embedding_model_dims": 768
            }
        },
        "graph_store": {
            "provider": "neo4j",
            "config": {
                "url": "env:NEO4J_URI",
                "username": "env:NEO4J_USERNAME",
                "password": "env:NEO4J_PASSWORD",
                "embedding_model_dims": 768
            }
        },
        "version": "v1.1"
    }
}